{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.decentralizedrights.com/$1"
    },
    {
      "src": "/explorer/(.*)",
      "dest": "https://explorer.decentralizedrights.com/$1"
    },
    {
      "src": "/app/(.*)",
      "dest": "https://app.decentralizedrights.com/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs"
}
